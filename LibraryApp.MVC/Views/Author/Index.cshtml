@model List<Author>

<div class="text-center">
    <div class="display-6">Authors</div>
</div>

<div class="row">
    <div class="col-md-9 row my-3">
        @foreach (var author in @Model)
        {
            <partial name="AuthorCard" model="@author" id="@author.Id"></partial>
        }
    </div>
</div>